@using Bysoft.Optimizely.Models.ViewModels
@using Bysoft.Optimizely
@using Bysoft.Optimizely.Models.Pages

@model IPageViewModel<ServicePage>



<h1 @Html.EditAttributes(m => m.CurrentPage.MetaTitle)>
    @(Model.CurrentPage.MetaTitle ?? Model.CurrentPage.Name)
</h1>

<div class="row">
    <div class="span8 clearfix">
        @Html.PropertyFor(x => x.CurrentPage.MainBody)
    </div>
</div>

@Html.PropertyFor(x => x.CurrentPage.MainContentArea, new { CssClass = "row" })
